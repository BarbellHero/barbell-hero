{
  "name": "barbell-hero",
  "description": "A workout tracker that stays out of your way",
  "version": "0.0.0",
  "homepage": "",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Martin Fracker, Jr.",
    "email": "martin.frackerjr@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": "^10.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "test": "yarn run lint && yarn run mocha",
    "build": "rimraf dist && cross-env NODE_ENV=production nuxt build && babel src --out-dir dist",
    "lint": "yarn run lint:run | snazzy",
    "lint:run": "standard",
    "lint:fix": "yarn run lint:run --fix",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node src/",
    "start": "cross-env NODE_ENV=production node dist/",
    "mocha": "mocha test/ --recursive --exit"
  },
  "dependencies": {
    "@feathersjs/configuration": "^2.0.4",
    "@feathersjs/errors": "^3.3.4",
    "@feathersjs/express": "^1.2.7",
    "@feathersjs/feathers": "^3.2.3",
    "@feathersjs/socketio": "^3.2.6",
    "compression": "^1.7.3",
    "consola": "^1.4.3",
    "cors": "^2.8.4",
    "feathers-sequelize": "^3.1.2",
    "helmet": "^3.13.0",
    "nuxt": "^2.1.0",
    "pg": "^7.5.0",
    "sequelize": "^4.38.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.6.1",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "rimraf": "^2.6.2",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "standard-loader": "^6.0.1"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "parser": "babel-eslint"
  }
}
